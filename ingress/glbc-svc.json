{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "default-http-backend",
    "labels": {
      "k8s-app": "glbc"
    }
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "port": 80,
        "targetPort": 8080,
        "protocol": "TCP",
        "name": "http"
      }
    ],
    "selector": {
      "k8s-app": "glbc"
    }
  }
}