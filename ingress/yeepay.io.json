{
  "apiVersion": "extensions/v1beta1",
  "kind": "Ingress",
  "metadata": {
    "name": "test"
  },
  "spec": {
    "rules": [
      {
        "host": "api.yeepay.com",
        "http": {
          "paths": [
            {
              "backend": {
                "serviceName": "es",
                "servicePort": 9200
              },
              "path": "/es"
            },
            {
              "backend": {
                "serviceName": "kibana",
                "servicePort": 5601
              },
              "path": "/kibana"
            }
          ]
        }
      }
    ]
  }
}
